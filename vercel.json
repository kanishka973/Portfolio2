{
  "version": 2,
  "buildCommand": "cd Portfolio-Builder && npm run build",
  "outputDirectory": "Portfolio-Builder/dist/public",
  "installCommand": "cd Portfolio-Builder && npm install",
  "builds": [
    {
      "src": "api/index.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.ts"
    }
  ]
}

